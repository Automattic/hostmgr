<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnvironmentVariables</key>
	<dict>
		<key>PATH</key>
		<string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/bin</string>
	</dict>
	<key>Label</key>
	<string>com.automattic.hostmgr.sync</string>
	<key>Program</key>
	<string>/usr/local/bin/hostmgr</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/hostmgr</string>
		<string>sync</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/usr/local/var/log/hostmgr-sync-stderr.log</string>
	<key>StandardOutPath</key>
	<string>/usr/local/var/log/hostmgr-sync-stdout.log</string>
	<key>StartInterval</key>
	<integer>60</integer>
	<key>WorkingDirectory</key>
	<string>/usr/local/var/</string>
</dict>
</plist>
