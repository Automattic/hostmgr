<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>com.automattic.hostmgr.git-mirror-server</string>
	<key>Program</key>
	<string>/usr/local/bin/caddy</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/caddy</string>
		<string>file-server</string>
		<string>--listen</string>
		<string>:41362</string>
		<string>--browse</string>
		<string>--root</string>
		<string>/usr/local/var/git-mirrors</string>
	</array>
	<key>RunAtLoad</key>
	<false/>
	<key>StandardErrorPath</key>
	<string>/usr/local/var/log/git-mirror-server-stderr.log</string>
	<key>StandardOutPath</key>
	<string>/usr/local/var/log/git-mirror-server-stdout.log</string>
	<key>WorkingDirectory</key>
	<string>/usr/local/var/</string>
</dict>
</plist>
